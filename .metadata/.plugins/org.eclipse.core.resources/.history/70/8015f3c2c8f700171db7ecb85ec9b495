import java.util.ArrayList;
import java.util.HashMap;
import java.util.Map;



public class Anagram {
	public static ArrayList<Integer> anagram(int[] a,int[] b) {
		ArrayList result = new ArrayList(a.length) ;
		for(int i = 0; i< a.length; i++) {
			for(int j = 0; j< b.length; j++ ) {
				if(a[i] == b[j]) {
					result.add(i,j);
				}
			}
		}
		return result;
	}
	
	public static ArrayList<Integer> anagram1(int[] a, int[] b) {
	Map D = new HashMap<>();
	for(int i = 0; i < b.length ; i++) {
		D.put(b[i], i);
	}
	ArrayList result = new ArrayList(a.length) ;
	int i = 0;
	for(int X: a) {
		result.add(i++,D.get(X));
	}
	return result;
	}
	
	public static void main(String[] args) {
		int a[] = {3,2,1};
		int b[] = {2,1,3};
		long startTime = System.currentTimeMillis();	
		ArrayList result = anagram(a,b);
		long endTime = System.currentTimeMillis();	
		System.out.print("result of 1st method : "+result);
		System.out.println("Time complexity of 1st method : " +(endTime-startTime));

		
		long startTime1 = System.currentTimeMillis();
		ArrayList result1 = anagram1(a,b);
		long endTime1 = System.currentTimeMillis();	
		System.out.print("result of 1st method : "+result1);
		System.out.println("Time complexity of 2nd method" +(endTime1-startTime1));

	}
}

